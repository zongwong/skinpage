.fr{float:right;}
.fl{float:left;}
.autowidth{width:auto!important;}
.dangerbg{background:#d9534f!important;margin:0 0 0 10px!important;}
.red{color:#f00;}
.maincolor{color:#2499F3;}
.whitebg{background:#fff;}
.clearfix:after,.clearfix:before{content:'';display:block;clear:both;}
.divborder{position:relative;}
.divborder:after{content:" ";position:absolute;left:0;bottom:0;right:0;height:1px;border-top:1px solid #d9d9d9;color:#d9d9d9;-webkit-transform-origin:0 0;transform-origin:0 0;-webkit-transform:scaleY(0.5);transform:scaleY(0.5);left:15px;}
.card{background:#fff;border:1px solid #eee;border-radius:6px;box-shadow:0 1px 3px rgba(0,37,55,0.05);margin:0 0.6rem;}
.no-afterborder:after{border-bottom:none;}
.no-beforeborder:before{border-top:none;}
.textright{text-align:right;}
.fixedbottom{position:fixed;bottom:0;z-index:1;left:50%;transform:translateX(-50%);-webkit-transform:translateX(-50%);}
.marginBottom50{margin-bottom:50px!important;}
body{background:#f2f2f2;font-family:-apple-system,BlinkMacSystemFont,"微软雅黑",Helvetica,Arial,sans-serif;}
ul,li{list-style:none;}
.banner{height:ren(426);overflow:hidden;}
.banner img{display:block;width:100%;}
.nav{display:flex;}
.nav .nav_item{flex:1;color:#666;}
.nav .nav_item.on{color:#2499f3;}
.nav .nav_item.on .nav_icon_ds{background-image:url("../imgs/ds.png");}
.nav .nav_item.on .nav_icon_jl{background-image:url("../imgs/jl.png");}
.nav .nav_item.on .nav_icon_yx{background-image:url("../imgs/yx.png");}
.nav .nav_icon{width:1.95rem;height:1.95rem;background:url("../imgs/ds2.png") center center/100% no-repeat;margin:1.25rem auto 0.55rem;}
.nav .nav_icon_jl{background-image:url("../imgs/jl2.png");}
.nav .nav_icon_yx{background-image:url("../imgs/yx2.png");}
.nav .nav_text{text-align:center;font-size:0.65rem;}
.list{margin:1.5rem 0.6rem 0 0.6rem;}
.list_title{padding-left:0.75rem;line-height:1.9rem;font-weight:600;}
.list_title .all_money{color:#f00;margin-left:0.5rem;}
.itembox div:last-child:after{border:none;}
.list_item{padding:1.4375rem 0.75rem;display:flex;}
.img_box{border-radius:50%;overflow:hidden;height:2.5rem;width:2.5rem;}
.img_box img{display:block;width:100%;}
.info_box{height:2.5rem;line-height:1.25rem;padding-left:0.5rem;flex-grow:1;}
.info_box div:nth-child(1){font-size:0.9rem;font-weight:bold;color:#09144c;}
.info_box div:nth-child(2){font-size:0.65rem;}
.info_box div:nth-child(2) .staff_no{color:#a8a8a8;}
.info_box div:nth-child(2) .staff_money em{color:#f00;font-style:normal;}
.center{margin-top:0.65rem;}
.editpaw{margin-top:1.125rem;padding:1.45rem;color:#999;}
.editpaw input::-webkit-input-placeholder{color:#999;}
.form_input{border:1px solid #eee;border-radius:4px;margin-bottom:0.7rem;}
.form_button{background:#2499f3;margin-top:1.75rem;}
.form_button:not(.weui-btn_disabled):active{background-color:#2694e8;}
.staff_code{width:1.35rem;height:1.35rem;}
.delete_staff{width:4rem;height:1.35rem;margin:0;display:inline-block;line-height:1.35rem;font-size:12px;margin-left:0.675rem;float:right;}
.staff_p2{text-align:right;margin-top:0.5rem;}
.staff_card{margin-bottom:50px;}
.staff_card .staff_money{font-size:0.65rem;color:#333;}
.staff_card .staff_money em{color:#f00;font-style:normal;font-weight:normal;}
.add_staff{width:78%;}
.staff_detail{margin-bottom:50px;}
.staff_detail div.date{font-weight:normal;}
.staff_detail .today_money{position:absoulte;right:0;bottom:0;}
.staff_detail .today_money .staff_money{color:#f00;font-weight:bold;}
.footer_money{width:100%;height:2.45rem;line-height:2.45rem;text-align:center;background:#fff;display:flex;font-size:0.8rem;}
.footer_money .has_closing{width:57%;}
.footer_money .closing{color:#fff;background:#55c8e5;flex-grow:1;}
.footer_money .closing:active{background-color:#52bcd7;}
.jsbox{text-align:center;}
.jsinput{border:0;outline:0;width:100px;}
.my_earnings{display:flex;text-align:center;line-height:1;padding:1.75rem 0;}
.my_earnings .my_money{flex:1;color:#ff7b7b;font-size:2.15rem;font-family:'helvetica';}
.my_earnings .history_money{flex:1;color:#2499f3;font-size:2.15rem;font-family:'helvetica';}
.my_earnings .sub_tips{color:#999;font-size:0.6rem;margin-top:0.45rem;}
.star_box{float:right;width:5.75rem;}
.star_item{float:left;width:0.75rem;height:1.125rem;margin:0 0.2rem;background:url("../imgs/star_light.png") 0 center/100% no-repeat;}
.star_item.night{background-image:url("../imgs/star_night.png");}
.staff_comment{margin-top:0.6rem;}
.star_status{height:2.5rem;background:#fff;text-align:center;padding:0.5rem;box-sizing:border-box;display:flex;}
.star_status div{flex:1;}
.star_status .star_btn{line-height:1.5rem;font-size:12px;float:left;border-radius:30px;border:1px solid #666;padding:0 0.375rem;width:2.25rem;color:#666;box-sizing:border-box;}
.star_status .star_btn.on{color:#fff;background:#2499f3;border-color:#2499f3;}
.pj_item{flex-wrap:wrap;}
.comment{padding-left:3rem;box-sizing:border-box;margin-top:0.4rem;font-size:0.7rem;color:#999;text-align:justify;word-break:break-all;word-wrap:break-word;width:100%;}
.agent_header{display:flex;background:#2499f3;text-align:center;font-size:0.65rem;height:2.2rem;line-height:2.2rem;}
.agent_header_item{flex:1;color:#f2f2f2;}
.agent_header_item.on{font-size:0.75rem;color:#fff;line-height:2.15rem;position:relative;}
.agent_header_item.on:after{content:'';position:absolute;width:0;height:0;bottom:0;left:50%;margin-left:-0.25rem;border-left:0.25rem solid transparent;border-right:0.25rem solid transparent;border-bottom:0.25rem solid #fff;}
.agent_nav .nav_icon_yy{background-image:url("../imgs/yy.png");}
.agent_nav .nav_icon_sh{background-image:url("../imgs/sh.png");}
.agent_nav .nav_icon_wtg{background-image:url("../imgs/wtg.png");}
.agent_nav .nav_item.on .nav_icon_yy{background-image:url("../imgs/yy2.png");}
.agent_nav .nav_item.on .nav_icon_sh{background-image:url("../imgs/sh2.png");}
.agent_nav .nav_item.on .nav_icon_wtg{background-image:url("../imgs/wtg2.png");}
.sj_phone{padding-left:0.75rem;background:url("../imgs/tell.png") 0 center no-repeat;background-size:0.625rem 0.6rem;}
.agent_img{border-radius:6px;}
.mycenter{background:url("../imgs/mycenter.png") 0 0/100% no-repeat;width:3.35rem;height:3.35rem;position:fixed;right:0.5rem;bottom:7.5rem;z-index:99;}
.agent_money{margin-top:1rem;}
.agent_list{margin-top:0.9rem;}
.staff_container{background:#fdf2db;overflow-x:hidden;}
.staff_container header{overflow:hidden;}
.staff_container header img{width:100%;display:block;}

