.header{position:relative;overflow:hidden;}
.mer_name{position:absolute;left:50%;top:7px;transform:translateX(-50%);-webkit-transform:translateX(-50%);background:#f8b500;color:#333;text-align:center;border-radius:6px;line-height:1.625rem;padding:0 0.75rem;z-index:11;white-space:nowrap;}
.mer_name .dot{width:6px;height:6px;position:absolute;background:#835008;border-radius:50%;top:3px;}
.mer_name .dot.right{right:.5rem;}
.mer_name .dot.left{left:.5rem;}
.mer_name .dot:after{content:"";width:1px;height:14px;background:#835008;position:absolute;left:50%;top:-150%;transform:translateX(-50%);-webkit-transform:translateX(-50%);}
.dashang_btn{background:#fdf2db;border-radius:8px;border:1px solid #facc4d;color:#fd7a1a;text-align:center;box-sizing:border-box;}
.dashang_btn.onon{background:#aa4b05;border:1px solid #aa4b05;color:#fdf2db;}
.onon::-webkit-input-placeholder{color:#fdf2db!important;}
.borad_box{padding:0 0.375rem;}
.ds_borad{border-radius:10px;background:#fd7a1a;padding:0.7rem 0.55rem;}
.ds_borad .ds_borad_title{color:#fdf2db;font-weight:bold;border-left:3px solid #fdf2db;line-height:1;padding-left:0.25rem;position: relative;}
.ds_borad .dsbtn_group{display:flex;flex-wrap:wrap;justify-content:space-around;}
.ds_borad .dsbtn_group *{width:4.75rem;margin-top:0.55rem;line-height:1.35rem;}
.ds_borad .dsbtn_group input{outline:none;}
.ds_borad .dsbtn_group .dashang_btn::-webkit-input-placeholder{color:#fd7a1a;}
.ds_borad .pjbtn_group *{padding:0 0.65rem;margin-top:0.55rem;float:left;margin-right:0.25rem;}
.pj_borad{background:#31b309;}
.pj_btn{background:#fdf2db;color:#31b309;}
.pj_btn.onon{background:#1f6c07;color:#fdf2db;border:1px solid #1f6c07;}
.pj_star-text{color: #fdf2db;float: right;font-size: 0.7rem;font-weight: normal;}
.ds-bg{background:#fcdfb6;padding:0.75rem 0.375rem;}
.pj-bg{background:#fcc67a;padding:0.75rem 0.375rem;}
.staff_avator{width:5rem;height:5rem;position:relative;margin:.5rem auto 1rem auto;}
.staff_avator>img{display:block;width:100%;height:100%;border-radius:50%;}
.staff_avator .staff_number{position:absolute;height:1.175rem;line-height:1.175rem;text-align:center;bottom:-.6rem;left:50%;transform:translateX(-50%);-webkit-transform:translateX(-50%);z-index:11;background:#ffd754;color:#8c6f0f;padding:0 1rem;border-radius:8px;white-space:nowrap;}
@media (max-width:370px){
  .ds_borad .dsbtn_group *{width:25.3%;}
}
.ds_textarea{outline:none;width:100%;margin-top:0.55rem;text-align:left;padding:0.25rem 0.5rem;height:100px;color:#31b309;}
.ds_textarea.on{background:#1f6c07;border:1px solid #1f6c07;color:#fdf2db;}
.ds_textarea .on::-webkit-textarea-placeholder{color:#feecba!important;}
.hr{width:100%;height:0;border-bottom:1px dashed #fff7e3;margin:0.3rem 0;}
.dsbtn{display:block;line-height:1.8rem;width:90%;font-size:0.9rem;margin:1rem auto;background:#C7343A;color:#fff;box-sizing: border-box;}
.dsbtn:active{background:#a43237;}
.spantext{float:left;}
.star_group{margin-top:-8px;margin-left:2rem;position: absolute;}
.star_group .pj_star{display:inline-block;width:1.4rem;height:1.4rem;background:url("../imgs/staff_star2.png") center center/100% no-repeat;}
.star_group .pj_star.on{background-image:url("../imgs/staff_star.png");}
.calendar_icon{float:right;height:100%;width:1.2rem;background:url("../imgs/calendar.png") center center/100% no-repeat;text-indent:-999px;overflow:hidden;margin-right:0.75rem;}
.weui-photo-browser-modal{z-index:999;}
.weui-photo-browser-modal .photo-container img{margin:auto;}
.toolbar .picker-button{color:#2499f3;}
.weui-search-bar__cancel-btn{color:#2499f3;}
.weui-dialog__btn{color:#2499f3;}
.staff_avatar{height:7.5rem;margin:0;}
.staff_avatar img{width:3.125rem;height:3.125rem;border-radius:50%;display:block;margin:1.35rem auto 0;}
.staff_avatar .staff_name{text-align:center;margin-top:0.75rem;font-size:0.9rem;line-height:1;color:#09144c;}
.money_banner{text-align:center;color:#fff;line-height:1;background:linear-gradient(to right,#42acfe,#2b9ef6);overflow:hidden;position:relative;}
.money_banner .return_icon{position:absolute;top:0;right:0;padding:0.875rem 0.625rem;}
.money_banner .return_icon span{display:block;width:0.95rem;height:1rem;background:url("../imgs/return_icon.png") center center/100% no-repeat;}
.money_banner p:nth-child(1){font-size:1rem;margin-top:2.05rem;margin-bottom:1.05rem;}
.money_banner p:nth-child(2){font-size:3.125rem;}
.money_banner p:nth-child(3){font-size:0.75rem;margin-top:2.1rem;margin-bottom:0.6rem;display:flex;}
.money_banner p:nth-child(3) span{flex:1;}
.money_banner p:nth-child(4){font-size:1.5rem;margin-bottom:2.25rem;display:flex;}
.money_banner p:nth-child(4) span{flex:1;}
.dashang_box{padding:1rem;padding-bottom:5rem;}
.dashang_box>p{margin-bottom:0.75rem;color:#000;}
.dashang_list li{display:flex;color:#999;}
.dashang_list li:first-child{color:#000;margin-bottom:0.5rem;}
.dashang_list li span{text-align:center;flex:1;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;}
.dashang_list li span:first-child{text-align:left;flex:2;}
.make_money{background:linear-gradient(to right,#42acfe,#2b9ef6);padding:1.25rem 0;position:fixed;bottom:0;left:0;width:100%;}
.make_money>a{color:#fff;display:block;border:1px solid #fff;text-align:center;margin:0 auto;width:14.5rem;border-radius:8px;line-height:2rem;}
.inputWrap{background:#fafafa;border:1px solid #e9ebed;width:85%;height:1.7rem;padding:0.25rem 0.5rem;box-sizing:border-box;display:inline-block;margin-right:10px;}
.inputWrap>input{text-align:center;width:80%;}
.yuan{color:#000;}

/*补充*/
.staff_moneylist li span{font-size:12px;line-height:2;}
.sure_btn{padding:0;font-size:12px;line-height:2;}
.pstips{color:#999;font-size:12px;padding:0 15px;width:100%;position:fixed;bottom:30px;left:0;box-sizing:border-box;}
.agent_infoBox{padding:6px 0;}
.item_fr{float:right;color:#999;}
.moneyBox{margin-right:20px;}